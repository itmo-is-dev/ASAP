{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs\\AppLogs.log"
        }
      }
    ]
  },
  "AllowedHosts": "*",

  "GoogleIntegration": {
    "GoogleDriveId": ""
  },

  "CacheOptions": {
    "CacheConfiguration": {
      "SizeLimit": 3600,
      "Expiration": "0:01:00"
    },
    "CacheEntryConfiguration": {
      "EntrySize": 10,
      "SlidingExpiration": "0:01:00",
      "AbsoluteExpiration": "0:01:00"
    }
  },

  "GithubIntegration": {
    "Auth": {
      "OAuthClientId": null,
      "OAuthClientSecret": null
    },
    "AppSettings": {
      "GithubAppSecret": null,
      "PrivateKeySource": "shreks-bot.pem",
      "AppIntegrationId": null,
      "JwtExpirationSeconds": 60
    }
  },

  "Identity": {
    "IdentityConfiguration": {
      "Secret": null,
      "Issuer": null,
      "Audience": null,
      "ExpiresHours": 1
    },
    "DefaultAdmins": [
      {
        "Username": null,
        "Password": null
      }
    ]
  },

  "TestEnvironment": {
    "Organization": null,
    "TemplateRepository": "Template",
    "Users": [
      null
    ]
  }
}

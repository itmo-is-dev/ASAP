{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs\\AppLogs.log"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  
  "PostgresConfiguration": {
    "Host": "rc1a-e2i3ytho3y1r9cri.mdb.yandexcloud.net",
    "Port": 6432,
    "Username": "shrek",
    "Password": "",
    "SslMode": "Prefer"
  },

  "GoogleIntegrationConfiguration": {
    "EnableGoogleIntegration": true
  },

  "CacheConfiguration": {
    "SizeLimit": 3600,
    "Expiration": "0:01:00",
    "CacheEntryConfiguration": {
      "EntrySize": 10,
      "SlidingExpiration": "0:01:00",
      "AbsoluteExpiration": "0:01:00"
    }
  },

  "GithubIntegrationConfiguration": {
    "GithubAuthConfiguration": {
      "OAuthClientId": null,
      "OAuthClientSecret": null
    },
    "GithubAppConfiguration": {
      "JwtExpirationSeconds": 60
    }
  },

  "Identity": {
    "IdentityConfiguration": {
      "Secret": null,
      "Issuer": "Kysect",
      "Audience": "Admins",
      "ExpiresHours": 1
    }
  }
}
